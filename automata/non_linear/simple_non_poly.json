{
  "automaton": {
    "var": "x",
    "mode": [
      {
        "id": 1,
        "eq": "x[1] = 1 / x[0]"
      },
      {
        "id": 2,
        "eq": "x[1] = -0.1 * x[0]"
      }
    ],
    "edge": [
      {
        "direction": "1 -> 2",
        "condition": "x >= 10",
        "reset": {
          "x": ["x - 1"]
        }
      },
      {
        "direction": "2 -> 1",
        "condition": "x <= 3"
      }
    ]
  },
  "init_state": [

      {"mode": 1, "x": [5.9]},
      {"mode": 1, "x": [7.5]},
      {"mode": 1, "x": [6.1]},
      {"mode": 2, "x": [6.3]},
      {"mode": 2, "x": [5.7]},
      {"mode": 2, "x": [4.9]},

      {"mode": 1, "x": [6.9]},
      {"mode": 1, "x": [5.3]},
      {"mode": 2, "x": [4.9]},
      {"mode": 1, "x": [7.5]},
      {"mode": 2, "x": [4.7]},
      {"mode": 1, "x": [5]},
      {"mode": 1, "x": [6]},
      {"mode": 2, "x": [7]},
      {"mode": 2, "x": [8]}
  ],
  "config": {
    "dt": 0.01,
    "total_time": 60.0,
    "order": 1,
    "need_reset": true,
    "other_items": "1 / x[?]",
    "svm_c": 1e5,
    "resampling_interval": 20,
    "window_size": 12,
    "truncation_size": 5,
    "change_th": 0.06,
    "n_sample_ratio": 1500.0,
    "sigma_process": 0.1,
    "sigma_measure": 0.01,
    "fitting_method": "lstsq",
    "clustering_th": 1.05
  }
}