{
  "automaton": {
    "var": "x1, x2",
    "mode": [
      {
        "id": 1,
        "eq": "x1[1] = x2[0] , x2[1] = - 6.2 * x2[0] - 10 * sin(x1[0])"
      },
      {
        "id": 2,
        "eq": "x1[1] = x2[0], x2[1] =  - 6.2 * 3 * x2[0] - 10 * sin(x1[0])"
      }
    ],
    "edge": [
      {
        "direction": "1 -> 2",
        "condition": "cos(x1) - x2/10 - 0.7 >= 0"
      },
      {
        "direction": "2 -> 1",
        "condition": "cos(x1) - x2/10 - 0.7 <= 0"
      }
    ]
  },
  "init_state": [
    {"mode": 1, "x1": [-0.878], "x2": [3.12]},
    {"mode": 1, "x1": [-0.892], "x2": [3.22]},
    {"mode": 1, "x1": [-0.810], "x2": [3.48]},
    {"mode": 1, "x1": [-0.860], "x2": [3.30]},
    {"mode": 1, "x1": [-0.825], "x2": [3.40]},
    {"mode": 1, "x1": [-0.865], "x2": [3.18]}

  ,
    {"mode": 1, "x1": [-0.815], "x2": [3.45]},
    {"mode": 1, "x1": [-0.876], "x2": [3.25]},
    {"mode": 1, "x1": [-0.822], "x2": [3.05]},
    {"mode": 1, "x1": [-0.800], "x2": [3.50]},
    {"mode": 1, "x1": [-0.880], "x2": [3.28]},
    {"mode": 1, "x1": [-0.835], "x2": [3.42]},
    {"mode": 1, "x1": [-0.868], "x2": [3.15]}
  ],
  "config": {
    "dt": 0.001,
    "total_time": 1,
    "order": 1,
    "other_items": "x_[?]; sin(x[?]); sin(x_[?])",
    "kernel": "rbf",
    "resampling_interval": 5,
    "truncation_size": 2,
    "svm_c":1e3,
    "n_sample_ratio":3,
    "change_th": 0.026,
    "window_size": 15,
    "sigma_process": 0.1,
    "sigma_measure": 0.005,
    "fitting_method": "lstsq",
    "random_seed": 3493675616
  }
}